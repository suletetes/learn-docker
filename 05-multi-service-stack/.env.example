# Example environment file for local development (05-multi-service-stack)
# Copy to .env and adjust values as needed.

# Redis (service name used in docker-compose)
REDIS_HOST=redis
REDIS_PORT=6379

# Postgres
PGUSER=postgres
PGHOST=postgres
PGDATABASE=postgres
PGPASSWORD=postgres
PGPORT=5432

# App-specific
SESSION_SECRET=changeme
# Ports (optional - verify compose mappings)
CLIENT_PORT=3000
SERVER_PORT=5000
NGINX_PORT=80

